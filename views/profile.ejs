<!doctype html>
<html>
<head>
	<title>Node Authentication</title>
	<link rel="stylesheet" href="/stylesheets/style.css">
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css">
	<link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
	<style>
		body 		{ padding-top:80px; word-wrap:break-word; }
	</style>
</head>
<body>

<div class="container">

	<div class="page-header text-center">
		<h1><span class="fa fa-anchor"></span> Velkommen til Dashboard</h1>
		<a href="/logout" class="btn btn-default btn-sm">Logout</a>
	</div>

	<div class="row">
		<div class="col-sm-12">
			<div class="well">
				<h3><span class="fa fa-user"></span> Opret, Rediger & Slet Produkter</h3>
			</div>
			<!-- her sÃ¦ttes tingene til at oprette og redigere samt slette produkter -->

		</div>
		
		<!-- LOCAL INFORMATION -->
		<div class="col-sm-4">
			<div class="well">
				<h3><span class="fa fa-user"></span> Local Bruger Profil</h3>
			</div>
					<p>
						<strong>id</strong>: <%= user.id %><br>
						<strong>username</strong>: <%= user.username %><br>
						<strong>password</strong>: <%= user.password %>
					</p>
		</div>
	</div>
</div>
</body>
